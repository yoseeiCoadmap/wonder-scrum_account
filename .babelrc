{
  "plugins": [
    [
      "react-css-modules",
      {
        "filetypes": {
          ".scss": {
            "syntax": "postcss-scss"
          }
        }
      }
    ],
    [
      "i18next-extract",
      {
        "locales": ["ja", "en"],
        "outputPath": "./src/application/configs/locales/{{locale}}.json"
      }
    ]
  ],
  "presets": [
    "react-app"
  ]
}
