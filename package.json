      "react-app",
      "react-app/jest"
    ]
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.scss": [
      "prettier --write",
      "stylelint --cache --color --fix",
      "git add"
    ]
  }
